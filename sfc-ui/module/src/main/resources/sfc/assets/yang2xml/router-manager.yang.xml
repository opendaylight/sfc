<?xml version="1.0" encoding="UTF-8"?>
<module name="router-manager"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:manager="urn:cisco:params:xml:ns:yang:router-manager"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types">
  <yang-version value="1"/>
  <namespace uri="urn:cisco:params:xml:ns:yang:router-manager"/>
  <prefix value="manager"/>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
    <revision-date date="2010-09-24"/>
  </import>
  <organization>
    <text>Cisco Systems</text>
  </organization>
  <contact>
    <text>Robert Gallas</text>
  </contact>
  <description>
    <text>Module intended for query of routers.</text>
  </description>
  <revision date="2013-12-13">
    <description>
      <text>first revision</text>
    </description>
  </revision>
  <rpc name="read-switches">
    <description>
      <text>Query Endpoints statuses</text>
    </description>
    <input>
      <leaf name="query">
        <type name="string"/>
        <description>
          <text>error by not providing input?</text>
        </description>
      </leaf>
    </input>
    <output>
      <list name="switches">
        <leaf name="switch">
          <type name="string"/>
        </leaf>
      </list>
    </output>
  </rpc>
</module>
