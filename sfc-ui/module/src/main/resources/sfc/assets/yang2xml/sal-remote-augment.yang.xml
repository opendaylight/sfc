<?xml version="1.0" encoding="UTF-8"?>
<module name="sal-remote-augment"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:salrmt-aug-ev-subscr="urn:sal:restconf:event:subscription">
  <yang-version value="1"/>
  <namespace uri="urn:sal:restconf:event:subscription"/>
  <prefix value="salrmt-aug-ev-subscr"/>
  <import module="sal-remote">
    <prefix value="salrmt"/>
    <revision-date date="2014-01-14"/>
  </import>
  <description>
    <text>Added input parameters to rpc create-data-change-event-subscription</text>
  </description>
  <revision date="2014-07-08"/>
  <augment target-node="/salrmt:create-data-change-event-subscription/salrmt:input">
    <leaf name="datastore">
      <type name="enumeration">
        <enum name="OPERATIONAL"/>
        <enum name="CONFIGURATION"/>
      </type>
    </leaf>
    <leaf name="scope">
      <type name="enumeration">
        <enum name="BASE"/>
        <enum name="ONE"/>
        <enum name="SUBTREE"/>
      </type>
    </leaf>
  </augment>
</module>
